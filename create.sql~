create table tb_home_appliances (name varchar(255) not null, average_monthly_consumption float8 not null, daily_use int4, monthly_use int4, power int4 not null, primary key (name));
insert into tb_home_appliances (name, power, monthly_use, daily_use, average_monthly_consumption) values ('Chuveiro elétrico 4500W', 4500, 30, 32, 72.0);
